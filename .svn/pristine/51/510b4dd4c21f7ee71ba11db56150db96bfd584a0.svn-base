public class AppInfo {
    public static final String DOWNLOAD = "下载";
    public static final String YI_DOWNLOAD = "已下载";
    public static final String UPDATE = "更新";
    public static final String OPEN = "打开";
    public String title;
    public int iconRes;
    public double size;//单位MB
    public int times;//下载次数
    public String status;//"下载" "已下载" "更新" "打开"
    public String time;//下载时间

    public AppInfo(int iconRes, String title, double size, int times, String status) {
        this.title = title;
        this.iconRes = iconRes;
        this.size = size;
        this.times = times;
        this.status = status;
    }

    public AppInfo(int iconRes, String title, double size, String time, String status) {
        this.title = title;
        this.iconRes = iconRes;
        this.size = size;
        this.time = time;
        this.status = status;
    }
}
