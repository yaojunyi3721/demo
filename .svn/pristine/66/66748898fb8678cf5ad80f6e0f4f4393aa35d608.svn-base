<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="20dp"
    android:orientation="vertical"
    tools:context="com.chinaitop.linxia.demo.activity.AnQuan.ChuLiangJiZhiLiangXinXiActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="10dp"
            android:layout_weight="3"
            android:gravity="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="@dimen/btn_height_35"
                android:layout_weight="1"
                android:gravity="center"
                android:text="库点:" />

            <TextView
                android:id="@+id/tv_kudian"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:background="@drawable/border" />

            <Button
                android:id="@+id/btn_kudian"
                android:layout_width="0dp"
                android:layout_height="@dimen/btn_height_35"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/border2"
                android:text="选择" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="10dp"
            android:layout_weight="3"
            android:gravity="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="@dimen/btn_height_35"
                android:layout_weight="1"
                android:gravity="center"
                android:text="品种:" />

            <TextView
                android:id="@+id/tv_pinzhong"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:background="@drawable/border" />

            <Button
                android:id="@+id/btn_pinzhong"

                android:layout_width="0dp"
                android:layout_height="@dimen/btn_height_35"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/border2"
                android:text="选择" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="10dp"
            android:layout_weight="3"
            android:gravity="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="@dimen/btn_height_35"
                android:layout_weight="1"
                android:gravity="center"
                android:text="性质:" />

            <TextView
                android:id="@+id/tv_xingzhi"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:background="@drawable/border" />

            <Button
                android:id="@+id/btn_xingzhi"

                android:layout_width="0dp"
                android:layout_height="@dimen/btn_height_35"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/border2"
                android:text="选择" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="10dp"
            android:layout_weight="3"
            android:gravity="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="@dimen/btn_height_35"
                android:layout_weight="1"
                android:gravity="center"
                android:text="等级:" />

            <TextView
                android:id="@+id/tv_dengji"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:background="@drawable/border" />

            <Button
                android:id="@+id/btn_dengji"

                android:layout_width="0dp"
                android:layout_height="@dimen/btn_height_35"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/border2"
                android:text="选择" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_margin="10dp"
            android:layout_weight="3"
            android:gravity="center">

            <TextView
                android:layout_width="0dp"
                android:layout_height="@dimen/btn_height_35"
                android:layout_weight="1"
                android:gravity="center"
                android:text="日期:" />

            <TextView
                android:id="@+id/tv_riqi"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:background="@drawable/border" />

            <Button
                android:id="@+id/btn_riqi"
                android:layout_width="0dp"
                android:layout_height="@dimen/btn_height_35"
                android:layout_marginLeft="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/border2"
                android:text="选择" />

        </LinearLayout>

    </LinearLayout>

    <include layout="@layout/food_card_title_layout"></include>

    <ListView
        android:id="@+id/list_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></ListView>

</LinearLayout>
