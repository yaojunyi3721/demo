public class ZhifaInfo {
    public String company;
    public String id;
    public String legalPerson;
    public String type;
    public String jiancharen;
    public String content;//检查内容
    public String question;//发现问题
    public String result;//检查结果
    public String area;//地区
    public boolean current;
    public String date;//检查日期

    public ZhifaInfo() {
    }

    public ZhifaInfo(String company, String id, String legalPerson, String type, boolean current) {
        this.company = company;
        this.id = id;
        this.legalPerson = legalPerson;
        this.type = type;
        this.current = current;
    }

    public ZhifaInfo(String company, String id, String legalPerson, String type) {
        this.company = company;
        this.id = id;
        this.legalPerson = legalPerson;
        this.type = type;
    }

    public ZhifaInfo(String date, String jiancharen, String company,String content, String question, String result) {
        this.company = company;
        this.jiancharen = jiancharen;
        this.content = content;
        this.question = question;
        this.result = result;
        this.date = date;
    }
}
